<UserControl x:Class="NetCore.Profiler.Extension.UI.TimelineCharts.ThreadTimelineChart"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:wpf="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <wpf:CartesianChart x:Name="LiveTimeline" Zoom="None" DisableAnimations="True" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        <wpf:CartesianChart.AxisY>
            <wpf:Axis ShowLabels="False" IsEnabled="False" MinValue="0" MaxValue="1" Position="LeftBottom" Visibility="Collapsed"/>
            <wpf:Axis ShowLabels="False" IsEnabled="False" MinValue="0" MaxValue="100" Position="RightTop" Visibility="Collapsed"/>
        </wpf:CartesianChart.AxisY>
        <wpf:CartesianChart.AxisX>
            <wpf:Axis ShowLabels="False" IsEnabled="False" MinValue="0"></wpf:Axis>
        </wpf:CartesianChart.AxisX>
    </wpf:CartesianChart>

</UserControl>
